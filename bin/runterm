#!/bin/sh


#urxvt-256color -fn "xft:ProggyCleanTT"
#urxvt-256color -fn "7x13,xft:Consolas"

FONT="xft:ProFont:antialias=true:pixelsize=13:hinting=true"
FONT="xft:xos4 Terminus:antialias=true:pixelsize=11:hinting=true"

FONT="xft:Cousine:antialias=true:pixelsize=11:hinting=true"
FONT_BOLD="${FONT}:bold"

exec urxvt-256color -fn "${FONT}" -fb "${FONT_BOLD}" $@
